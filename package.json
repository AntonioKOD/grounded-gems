{
  "name": "sacavia",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:production": "NODE_ENV=production next build",
    "start": "next start",
    "start:production": "NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next && rm -rf node_modules/.cache && rm -rf .swc",
    "clean:full": "rm -rf .next && rm -rf node_modules && npm ci",
    "fresh-build": "npm run clean && npm run build",
    "debug-auth": "NODE_ENV=development next dev",
    "build:mobile": "MOBILE_BUILD=true next build",
    "build:mobile:production": "NODE_ENV=production MOBILE_BUILD=true next build",
    "dev:mobile": "npx cap sync && npx cap open ios",
    "dev:mobile:android": "npx cap sync && npx cap open android",
    "sync:mobile": "npx cap sync",
    "add:ios": "npx cap add ios",
    "add:android": "npx cap add android",
    "open:ios": "npx cap open ios",
    "open:android": "npx cap open android",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "perf:lighthouse": "npx lighthouse http://localhost:3000 --output html --output-path ./lighthouse-report.html",
    "perf:bundle-analyzer": "ANALYZE=true npm run build",
    "test:load": "npx autocannon -c 10 -d 30 http://localhost:3000",
    "sync-categories": "node scripts/sync-foursquare-categories.js",
    "test-categories": "node test-category-sync.js",
    "fix-live-photos": "node scripts/fix-live-photos.js",
    "setup-firebase": "node scripts/setup-firebase-env.js",
    "test-firebase": "node scripts/test-firebase.js",
    "init-database": "tsx scripts/init-database.ts",
    "db:indexes": "tsx scripts/init-database.ts",
    "db:monitor": "tsx -e \"import('./lib/database-monitoring').then(m => m.initializeDatabaseMonitoring())\""
  },
